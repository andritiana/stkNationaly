        <preference name="AndroidWindowSplashScreenAnimationDuration" value="1000" />
        <preference name="AndroidWindowSplashScreenBackground" value="#40529D" />
        <resource-file src="precompilation-resources/android/onesignal/mipmap-mdpi/ic_stat_onesignal_default_foreground.png" target="app/src/main/res/drawable-mdpi/ic_stat_onesignal_default.png" />
