<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="50103" id="com.stknational.app" ios-CFBundleVersion="5.1.0" version="5.2.0-beta.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="FirebaseAnalytics">
        <param name="android-package" value="by.chemerisuk.cordova.firebase.FirebaseAnalyticsPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="StatusBar">
        <param name="android-package" value="org.apache.cordova.statusbar.StatusBar" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SQLitePlugin">
        <param name="android-package" value="io.sqlc.SQLitePlugin" />
    </feature>
    <feature name="OneSignalPush">
        <param name="android-package" value="com.onesignal.cordova.OneSignalPush" />
    </feature>
    <allow-navigation href="http://localhost/*" />
    <allow-navigation href="https://localhost/*" />
    <allow-navigation href="ionic://*" />
    <feature name="IonicWebView">
        <param name="android-package" value="com.ionicframework.cordova.webview.IonicWebView" />
    </feature>
    <name>MySTK</name>
    <description>Application de la section jeune de l'Eglise protestante malgache en France.</description>
    <author email="marc@stk-national.fr" href="https://stk.fpma.church/">STK National</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="https://stk.fpma.church/embeded/*" />
    <allow-navigation href="https://stk.fpma.church/*" />
    <allow-navigation href="https://*youtube.com/*" />
    <allow-navigation href="https://*soundcloud.com/*" />
    <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application" xmlns:android="http://schemas.android.com/apk/res/android">
        <application android:networkSecurityConfig="@xml/network_security_config" />
    </edit-config>
    <resource-file src="resources/android/xml/network_security_config.xml" target="app/src/main/res/xml/network_security_config.xml" />
    <resource-file src="google-services.json" target="app/google-services.json" />
    <allow-intent href="market:*" />
    <icon background="precompilation-resources/android/icon/drawable/ic_mystk_background.xml" density="mdpi" foreground="precompilation-resources/android/icon/drawable/ic_mystk_foreground.xml" src="precompilation-resources/android/icon/mipmap-mdpi/ic_mystk.png" />
    <icon background="precompilation-resources/android/icon/drawable/ic_mystk_background.xml" density="hdpi" foreground="precompilation-resources/android/icon/drawable/ic_mystk_foreground.xml" src="precompilation-resources/android/icon/mipmap-hdpi/ic_mystk.png" />
    <icon background="precompilation-resources/android/icon/drawable/ic_mystk_background.xml" density="xhdpi" foreground="precompilation-resources/android/icon/drawable/ic_mystk_foreground.xml" src="precompilation-resources/android/icon/mipmap-xhdpi/ic_mystk.png" />
    <icon background="precompilation-resources/android/icon/drawable/ic_mystk_background.xml" density="xxhdpi" foreground="precompilation-resources/android/icon/drawable/ic_mystk_foreground.xml" src="precompilation-resources/android/icon/mipmap-xxhdpi/ic_mystk.png" />
    <icon background="precompilation-resources/android/icon/drawable/ic_mystk_background.xml" density="xxxhdpi" foreground="precompilation-resources/android/icon/drawable/ic_mystk_foreground.xml" src="precompilation-resources/android/icon/mipmap-xxxhdpi/ic_mystk.png" />
    <resource-file src="precompilation-resources/android/onesignal/mipmap-mdpi/ic_stat_onesignal_default_foreground.png" target="app/src/main/res/drawable-mdpi/ic_stat_onesignal_default.png" />
    <resource-file src="precompilation-resources/android/onesignal/mipmap-hdpi/ic_stat_onesignal_default_foreground.png" target="app/src/main/res/drawable-hdpi/ic_stat_onesignal_default.png" />
    <resource-file src="precompilation-resources/android/onesignal/mipmap-xhdpi/ic_stat_onesignal_default_foreground.png" target="app/src/main/res/drawable-xhdpi/ic_stat_onesignal_default.png" />
    <resource-file src="precompilation-resources/android/onesignal/mipmap-xxhdpi/ic_stat_onesignal_default_foreground.png" target="app/src/main/res/drawable-xxhdpi/ic_stat_onesignal_default.png" />
    <resource-file src="precompilation-resources/android/onesignal/mipmap-xxxhdpi/ic_stat_onesignal_default_foreground.png" target="app/src/main/res/drawable-xxxhdpi/ic_stat_onesignal_default.png" />
    <resource-file src="precompilation-resources/android/onesignal/mipmap-xxxhdpi/ic_onesignal_large_icon_default_foreground.png" target="app/src/main/res/drawable-xxxhdpi/ic_onesignal_large_icon_default.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="GradlePluginGoogleServicesEnabled" value="true" />
    <preference name="GradlePluginGoogleServicesVersion" value="4.3.3" />
    <preference name="webView" value="com.ionicframework.cordova.webview.IonicWebViewEngine" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreenDelay" value="1500" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="android-targetSdkVersion" value="32" />
    <preference name="AndroidWindowSplashScreenAnimatedIcon" value="precompilation-resources/android/splash/animated_logo_432.xml" />
    <preference name="AndroidWindowSplashScreenAnimationDuration" value="1000" />
    <preference name="AndroidWindowSplashScreenBackground" value="#40529D" />
</widget>
